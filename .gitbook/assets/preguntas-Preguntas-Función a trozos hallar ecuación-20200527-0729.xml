        dragMode: Desmos.DragModes.NONE,
        color: Desmos.Colors.PURPLE
    });
    //TERCER TROZO		
    calculator.setExpression({
        id: 'graph3',
        latex: 'y={a}\\left \\{ x>{c}  \\right \\}',
        color: Desmos.Colors.BLUE
    });
    calculator.setExpression({
        id: 'pointE',
        latex: 'E=({c},{a})',
        pointStyle: Desmos.Styles.POINT,
        dragMode: Desmos.DragModes.NONE,
        color: Desmos.Colors.PURPLE
    });
